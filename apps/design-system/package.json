{
  "name": "design-system",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "pnpm content:build && pnpm build:registry && next build",
    "build:registry": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --log-level silent --write \"registry/**/*.{ts,tsx,mdx}\" --cache",
    "start": "next start",
    "lint": "next lint",
    "content:build": "contentlayer build",
    "typecheck": "contentlayer build && tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "contentlayer": "0.3.4",
    "fs": "^0.0.1-security",
    "jotai": "^2.8.0",
    "lucide-react": "^0.338.0",
    "markdown-wasm": "^1.2.0",
    "next": "14.1.4",
    "next-contentlayer": "0.3.4",
    "react": "^18.2.0",
    "react-docgen": "^7.0.3",
    "react-dom": "^18.2.0",
    "react-wrap-balancer": "^1.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.6.0",
    "rehype-slug": "^6.0.0",
    "remark": "^14.0.3",
    "remark-code-import": "^1.2.0",
    "remark-gfm": "^3.0.1",
    "unist-util-visit": "^5.0.0",
    "ts-morph": "^22.0.0",
    "lodash.template": "4.5.0",
    "ui": "*"
  },
  "devDependencies": {
    "@shikijs/compat": "^1.1.7",
    "@types/lodash.template": "4.5.0",
    "@types/node": "^20",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "mdast-util-toc": "^6.1.1",
    "postcss": "^8",
    "shiki": "^1.1.7",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "unist-builder": "3.0.0",
    "rimraf": "^4.1.3",
    "config": "*"
  }
}
