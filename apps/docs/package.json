    "@radix-ui/react-accordion": "^1.1.2",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.13.4",
    "common": "*",
    "config": "*",
    "framer-motion": "^6.5.1",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "hast-util-has-property": "^2.0.1",
    "isbot": "^3.6.5",
    "jsrsasign": "^10.5.26",
    "lodash": "^4.17.21",
    "markdown-toc": "^1.2.0",
    "mdast": "^3.0.0",
