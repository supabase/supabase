    "@tanstack/react-query": "^5.13.4",
    "acorn": "^8.11.3",
    "acorn-typescript": "^1.4.13",
    "ai-commands": "workspace:*",
    "common": "workspace:*",
    "common-tags": "^1.8.2",
    "config": "workspace:*",
