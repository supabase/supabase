        "NEXT_PUBLIC_EXAMPLES_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_EXAMPLES_SUPABASE_URL",
        "NEXT_PUBLIC_USERCENTRICS_RULESET_ID",
